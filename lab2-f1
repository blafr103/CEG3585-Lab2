public class FourierSeriesCalculator {
  public static void main(String[] args) {
    double pi = Math.PI;
    double a0 = 0.0;
    double ab = 0.0;
    double sum = 0.0;
    
    for (int n = 1; n <= 100; n++) {
      double bn = (-20 * Math.cos(n * pi)) / (n * pi);
      sum += a0 + ab + bn;
    }
    
    System.out.println("Sum of Fourier series up to 100th harmonic: " + sum);
  }
}
